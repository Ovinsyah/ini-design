{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-react"],
    "jsRules": {},
    "rules": {
        "jsx-alignment": false,
        "no-submodule-imports": false,
        "no-implicit-dependencies": false,
        "jsx-no-multiline-js": false,
        "no-unused-expression": false,
        "jsx-no-lambda": false,
        "semicolon": [true, "always"]
    },
    "rulesDirectory": []
}
